{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--browserUrl",
        "http://127.0.0.1:9222"
      ]
    },
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        ".",
        "C:\\"
      ]
    },
    "prompt-library": {
      "type": "stdio",
      "command": "node",
      "args": [
        "C:/RH-OptimERP/MCPs/prompt-library-orchestrator/dist/index.js"
      ]
    },
    "team-sync": {
      "type": "stdio",
      "command": "node",
      "args": [
        "C:/RH-OptimERP/MCPs/team-sync/dist/index.js"
      ]
    },
    "qco": {
      "type": "stdio",
      "command": "node",
      "args": [
        "C:/RH-OptimERP/MCPs/quality-code-orchestrator/dist/index.js"
      ]
    }
  }
}